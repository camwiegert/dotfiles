[user]
	name = Cam Wiegert
	email = cam@camwiegert.com
	username = camwiegert
[core]
	excludesfile = $HOME/.gitignore
	editor = vim
[color]
    ui = auto
[alias]
    lo = log --oneline --graph
    br = branch
    st = status
    co = checkout
    ci = commit -v
    cm = commit -m
    df = diff -p --stat
    ec = config --global -e
[push]
	default = simple
[advice]
    statusHints = false
